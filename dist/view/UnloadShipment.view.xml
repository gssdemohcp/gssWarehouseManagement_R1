<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form"
	controllerName="gss.newWarehouseManage_R1.controller.UnloadShipment" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>unlByShip}">
				<customHeader>
					<Toolbar>
						<ToolbarSpacer/>
						<Title id="rfScreen" text="{i18n>unShiptit}" level="H2" textAlign="Center"/>
						<ToolbarSpacer/>
						<Button id="syuser" icon="sap-icon://account" type="Transparent" press="handleUserNamePress"/>
					</Toolbar>
				</customHeader>
				<Toolbar>
					<Breadcrumbs class="sapUiTinyMarginBegin" id="breadCrumbs"/>
				</Toolbar>
				<Toolbar id="toolbarheader">
					<Input class="sapUiTinyMarginBegin" id="inputValue" valueHelpRequest="onHandlePoHelp" submit="iGetInput" liveChange="onHandleBinding"
						width="100%" textAlign="Begin"/>
					<Button id="scanButton" icon="sap-icon://bar-code" press="onHandleScanInput" ariaLabelledBy="actionButtonLabel"/>
				</Toolbar>
				<content>
					<f:SimpleForm id="delvForm" editable="false" layout="ResponsiveGridLayout" title="{i18n>Delform1Title}" labelSpanXL="3" labelSpanL="3"
						labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1"
						columnsL="1" columnsM="1" singleContainerFullSize="false">
						<f:content>
							<HBox>
								<VBox>
									<Label class="sapUiTinyMarginTop" text="{i18n>stgingAreaTitle}"/>
									<Label class="sapUiTinyMarginTop" text="{i18n>doorTitle}"/>
								</VBox>
								<VBox>
									<Text class="sapUiTinyMarginTop sapUiTinyMarginBegin dataStyle" text="{itemList>/aItems/0/Lgbzo}"/>
									<Text class="sapUiTinyMarginTop sapUiTinyMarginBegin dataStyle" text="{itemList>/aItems/0/Lgtor}"/>
								</VBox>
							</HBox>
						</f:content>
					</f:SimpleForm>
					<f:SimpleForm id="delhuForm" editable="false" layout="ResponsiveGridLayout" title="{i18n>form2Title}" labelSpanXL="3" labelSpanL="3"
						labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1"
						columnsL="1" columnsM="1" singleContainerFullSize="false">
						<f:content>
							<Toolbar>
								<Input id="scanHUunDel" type="Text" valueHelpRequest="onHandlePoHelp" submit="iGetInput" placeholder="{i18n>placeholderTitle}" width="100%"
									textAlign="Begin" maxLength="20"/>
								<Button id="shu" tooltip="Bar Code scanner" icon="sap-icon://bar-code" press="onHandleScanHU" ariaLabelledBy="actionButtonLabel"/>
							</Toolbar>
							<HBox>
								<VBox>
									<Label id="unloadedLab" class="sapUiTinyMarginTop sapUiTinyMarginEnd" text="{i18n>unloadedHUTitle}"/>
									<!--<Label id="unloadLab" class="sapUiTinyMarginTop sapUiTinyMarginEnd" text="Unloaded HU" visible="false"/>-->
									<Label class="sapUiTinyMarginTop sapUiTinyMarginEnd" text="{i18n>totalHUTitle}"/>
									<Label id="unloadedWT" class="sapUiTinyMarginTop sapUiTinyMarginEnd" text="{i18n>unloadedWT}"/>
									<Label class="sapUiTinyMarginTop sapUiTinyMarginEnd" text="{i18n>totalWTTitle}"/>
								</VBox>
								<VBox>
									<Text id="loadedTxt" class="sapUiTinyMarginTop sapUiTinyMarginBegin dataStyle" text="{itemList>/aItems/0/UnloadedHu}"/>
									<Text id="totalHU" class="sapUiTinyMarginTop sapUiTinyMarginBegin dataStyle" text="{itemList>/aItems/0/TotalHu}"/>
									<Text class="sapUiTinyMarginTop sapUiTinyMarginBegin dataStyle" text="{itemList>/aItems/0/LoadedHuWt} {itemList>/aItems/0/WtUnit}"/>
									<Text class="sapUiTinyMarginTop sapUiTinyMarginBegin dataStyle" text="{itemList>/aItems/0/TotHuWt} {itemList>/aItems/0/WtUnit}"/>
								</VBox>
							</HBox>
						</f:content>
					</f:SimpleForm>
				</content>
				<footer id="footer"
					class="sapContrast sapContrastPlus sapMBar sapMBar-CTX sapMContent-CTX sapMFooter-CTX sapMIBar sapMIBar-CTX sapMPageFooter">
					<Bar id="footerbar" class="page_footer">
						<contentLeft>
							<Button id="back" icon="sap-icon://nav-back" press="onNavBack" tooltip="Back" visible="false"/>
							<Button id="message-popup" icon="sap-icon://message-popup" tooltip="Status" type="Emphasized" press="handleMessagePopoverPress"/>
						</contentLeft>
						<contentRight>
							<Button id="accept" class="accept sapUiTinyMargin sapUiResponsiveContentPadding button_Size" press="unload" icon="sap-icon://accept"/>
							<Button id="difference" press="onHandleDifference" visible="false" icon="sap-icon://sys-minus"/>
							<Button id="undoDel" class="undoDel sapUiTinyMargin sapUiResponsiveContentPadding button_Size" press="unloadRevert" icon="sap-icon://undo"/>
						</contentRight>
					</Bar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>